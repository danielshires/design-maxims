
function pageTransition () {
    
    var tl = gsap.timeline();

    tl.to ('')


}


function delay(n) {
n = n || 2000;
return new Promise(done => {
    setTimeout (() => {
        done();
    }, n);
});
    }

barba.init ({

    sync: true,

    transitions: [{

        async leave(data) {

            const done = this.async();

            pageTransition();
            await delay (1500);
            done();
        }
    }]
})